{
  "name": "@xiangheng08/qrcode",
  "version": "0.0.1",
  "author": "xiangheng08",
  "license": "MIT",
  "keywords": [
    "qrcode",
    "xiangheng08",
    "xiangheng08/qrcode",
    "@xiangheng08/qrcode"
  ],
  "homepage": "https://github.com/xiangheng08/xh-qrcode",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiangheng08/xh-qrcode"
  },
  "bugs": {
    "url": "https://github.com/xiangheng08/xh-qrcode/issues"
  },
  "license_url": "https://github.com/xiangheng08/xh-qrcode/blob/main/LICENSE",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/xh-qrcode.cjs",
  "module": "./dist/xh-qrcode.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/xh-qrcode.js",
      "require": "./dist/xh-qrcode.cjs"
    },
    "./utils": {
      "types": "./dist/utils/export.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./elements/group": {
      "types": "./dist/elements/group.d.ts",
      "import": "./dist/elements/group.js",
      "require": "./dist/elements/group.cjs"
    },
    "./elements/vcard": {
      "types": "./dist/elements/vcard.d.ts",
      "import": "./dist/elements/vcard.js",
      "require": "./dist/elements/vcard.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.lib.json && vite build",
    "preview": "vite preview",
    "type-check": "tsc -p tsconfig.lib.json --noEmit",
    "format": "prettier --w lib/ --w src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:type-check": "tsc -p tsconfig.docs.json"
  },
  "dependencies": {
    "dijkstrajs": "^1.0.3",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.8.1",
    "@xiangheng08/qrcode": "link:",
    "fast-glob": "^3.3.3",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.12",
    "vue": "^3.5.24"
  }
}